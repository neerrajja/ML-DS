import matplotlib.pyplot as plt
bldsugar_men=[113,85,90,150,149,88,93,115,135,801,77,82,129]
bldsugar_women=[67,98,89,120,133,150,84,69,89,79,120,112,100]
plt.xlabel('Sugar range')
plt.ylabel('Blood sugar analysis')
plt.hist([bldsugar_men,bldsugar_women],bins=[80,100,120,150],rwidth=0.95,color=['green','orange'],label=['men','women'])
plt.legend()
plt.show()
